{
  "name": "@datanest-earth/nodejs-client",
  "version": "0.2.3",
  "description": "Datanest API Client to easily create signed requests",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist/",
    "src/",
    "index.mjs",
    "readme.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --target node18",
    "test": "vitest run",
    "test:dev": "vitest",
    "clean": "rm -r dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/datanest-earth/datanest-nodejs-client.git"
  },
  "keywords": [
